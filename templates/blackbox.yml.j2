- labels:
    module: icmpcheck
  targets:
    {% for host in groups[prometheus_monitored_group] -%}
    - {{ hostvars_cache[host]['ansible_hostname'] }},{{ prometheus_blackbox_admin_ip }}=>{{ hostvars_cache[host]['prometheus_blackbox_ip'] }}
    {% endfor %}

- labels:
    module: tcpcheck
  targets:
    {% for host in groups[prometheus_monitored_group] -%}
    {%- set loop_next = groups[prometheus_monitored_group][0 if loop.last else loop.index] -%}
    {% for service in prometheus_tcpcheck_services -%}
    {% for port in hostvars_cache[loop_next]['bb_service_ports'][service] -%}
    - {{service}},{{ hostvars_cache[loop_next]['ansible_hostname'] }},{{ hostvars_cache[host]['prometheus_blackbox_ip'] }}=>{{ hostvars_cache[loop_next]['prometheus_blackbox_ip_openio'] }}:{{ port }}
    {% endfor %}
    {%- endfor %}
    {%- endfor %}

{% for service in prometheus_blackbox_services %}
- labels:
    module: "{{service}}"
  targets:
    {% for host in groups[prometheus_monitored_group] -%}
    {%- set loop_next = groups[prometheus_monitored_group][0 if loop.last else loop.index] -%}
    {% for port in hostvars_cache[loop_next]['bb_service_ports'][service] -%}
    {%- if service in ['meta0', 'meta1', 'meta2', 'conscience'] -%}
    - {{ hostvars_cache[loop_next]['ansible_hostname'] }},{{ hostvars_cache[host]['prometheus_blackbox_ip'] }}=>{{ hostvars_cache[loop_next]['prometheus_blackbox_ip_openio'] }}:{{ hostvars_cache[loop_next]['bb_service_ports']['oioproxy'][0] }},{{ hostvars_cache[loop_next]['prometheus_blackbox_ip_openio'] }}:{{ port }}
    {% else -%}
    - {{ hostvars_cache[loop_next]['ansible_hostname'] }},{{ hostvars_cache[host]['prometheus_blackbox_ip'] }}=>{{ hostvars_cache[loop_next]['prometheus_blackbox_ip_openio'] }}:{{ port }}
    {% endif -%}
    {% endfor %}
    {%- endfor %}

{% endfor %}
